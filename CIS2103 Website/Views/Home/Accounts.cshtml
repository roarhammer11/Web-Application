@{
    ViewData["Title"] = "Accounts";
    Layout = "_DashboardLayout";
}

<div id="searchbar-container" class="position-fixed">
    <div class="container h-100">
        <div class="h-100">
            <div class="searchbar d-flex justify-content-center">
                <p>Search</p>
                <input class="search_input" type="text" name="">
                <a href="#" class="search_icon"><i class="fas fa-search"></i></a>
            </div>
        </div>
    </div>
</div>

<div class="container-fluid">
    <div class="table-responsive">
        <table class="table table-bordered">
            <thead class="table-secondary">
                <tr>
                    <th scope="col" class="p-3">First Name</th>
                    <th scope="col" class="p-3">Last Name</th>
                    <th scope="col" class="p-3">Status</th>
                    <th scope="col" class="p-3">Privilege</th>
                    <th scope="col" class="p-3">Actions</th>
                </tr>
            </thead>
            <tbody id="accountsTable">
               
            </tbody>
        </table>
    </div>
</div>

